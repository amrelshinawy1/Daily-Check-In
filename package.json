{
  "name": "daily-check-in-mvp",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "e2e": "turbo run e2e",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@react-native-community/cli": "latest",
    "@turbo/gen": "^1.12.4",
    "prettier": "^3.1.1",
    "turbo": "^2.5.4"
  },
  "packageManager": "pnpm@10.12.4"
}
